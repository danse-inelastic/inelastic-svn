from vnf.components.DBObjectForm import DBObjectForm as base


class Block(base):


    class Inventory(base.Inventory):

        import pyre.inventory
        width = pyre.inventory.float( name = 'width' )
        height = pyre.inventory.float( name = 'height' )
        thickness = pyre.inventory.float( name = 'thickness' )

    parameters = ['short_description', 'width', 'height', 'thickness']
    DBTable = 'Block'


    def __init__(self, name = None):
        if name is None:
            name = 'block'

        base.__init__(self, name)

        return



def form(): return Block()

